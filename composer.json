{
	"name": "purplespider/basic-social-media",
	"description": "Adds social media URL fields to CMS and provides basic FontAwesome template",
	"type": "silverstripe-vendormodule",
	"homepage": "https://github.com/purplespider/silverstripe-basic-social-media",
	"keywords": ["silverstripe", "facebook", "social media"],
	"license": "BSD-3-Clause",
	"authors": [{
		"name": "James Cocker",
		"email": "ssmodules@pswd.biz"
	}],
	"support": {
		"issues": "https://github.com/purplespider/silverstripe-basic-social-media/issues"
	},
	"require": {
		"php": "^8.3 || ^8.4",
		"silverstripe/framework": "^6.0",
		"fromholdio/silverstripe-externalurlfield": "^2"
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"silverstripe/vendor-plugin": true
		}
	}
}
